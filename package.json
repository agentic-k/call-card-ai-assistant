{
  "name": "CallCard",
  "displayName": "CallCard",
  "author": "Call Card Team",
  "description": "Call Card is a desktop app that allows you to create and manage your calls.",
  "private": true,
  "version": "0.0.11",
  "repository": {
    "type": "git",
    "url": "https://github.com/call-card/call-card.git"
  },
  "type": "module",
  "main": "electron/main.mjs",
  "config": {
    "forge": "./forge.config.mjs"
  },
  "scripts": {
    "dev": "vite --mode development",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview",
    "electron:dev": "electron electron/main.mjs",
    "dev:electron": "dotenv -e .env.local -- concurrently \"BROWSER=none npm run dev\" \"wait-on tcp:127.0.0.1:8080 && VITE_DEV_SERVER_URL=http://127.0.0.1:8080 npm run electron:dev\"",
    "electron:build": "NODE_ENV=production vite build && electron-builder --config electron-builder.config.cjs",
    "electron:build:mac": "NODE_ENV=production vite build && electron-builder --config electron-builder.config.cjs --mac --arm64",
    "clean": "rm -rf out .vite dist release node_modules/.vite node_modules/.cache",
    "package": "npm run clean && NODE_ENV=production vite build --mode production && electron-forge package",
    "make": "npm run clean && NODE_ENV=production dotenv -e .env.production -- vite build --mode production && electron-forge make",
    "publish": "npm run clean && NODE_ENV=production dotenv -e .env.production -- vite build --mode production && electron-forge publish --platform=darwin",
    "create-icon": "node scripts/create-transparent-icon.js"
  },
